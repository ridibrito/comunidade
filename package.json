{
  "name": "comunidade",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "eslint",
    "security-audit": "node scripts/security-audit.js",
    "audit": "npm audit --audit-level=moderate",
    "audit-fix": "npm audit fix",
    "outdated": "npm outdated",
    "update-deps": "npm update",
    "check-security": "npm run security-audit && npm run audit",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "npm run analyze",
    "perf:audit": "npm run build && npm run analyze",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "perf:test": "npm run build && npm run start & sleep 5 && npm run perf:lighthouse",
    "performance-audit": "node scripts/performance-audit.js",
    "setup-env": "node scripts/setup-env.js",
    "test-improvements": "node scripts/test-improvements.js",
    "test-improvements-simple": "node scripts/test-improvements-simple.js",
    "test:all": "npm run test-improvements-simple && npm run check-security"
  },
